<%= render 'admin/admin_index' %>

<% @listings.each do |listing| %>
  <table>
  <% listing.attributes.each do |key, value| %>
    <tr>
      <td><%= key %></td>
      <td><%= value %></td>
    </tr>
  <% end %>
  </table>
  <br>
  <%= link_to 'More details', admin_listing_path(listing) %>
  <hr>
<% end %>
