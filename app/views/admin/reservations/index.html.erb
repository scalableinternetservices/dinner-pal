<%= render 'admin/admin_index' %>

<% @reservations.each do |reservation| %>
  <table>
  <% reservation.attributes.each do |key, value| %>
    <tr>
      <td><%= key %></td>
      <td><%= value %></td>
    </tr>
  <% end %>
  </table>
  <br>
  <%= link_to 'More details', admin_reservation_path(reservation) %>
  <hr>
<% end %>
